{
  "version": 2,
  "builds": [
    {
      "src": "react-eduapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "react-eduapp/dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/react-eduapp/dist/$1"
    }
  ]
}
